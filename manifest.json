{
    "manifest_version": 3,
    "name": "Bug_Extension",
    "version": "1.0.1",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["jquery.js","main.js","move.js"],
        "css": ["style.css"] 
        }
    ],
    "web_accessible_resources": [
        {
        "matches": ["<all_urls>"],
        "resources": ["assets/c0.png","assets/c1.png","assets/c2.png"]
        }
    ],
    "description": "A bug description"
}